{
  "name": "palma-ui",
  "version": "2.0.2",
  "description": "Component library built in React and using Material UI",
  "keywords": [
    "ui",
    "components",
    "material",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/luisott/palma-ui/issues"
  },
  "homepage": "https://github.com/luisott/palma-ui.git",
  "license": "MIT",
  "author": {
    "name": "Luis Silva"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luisott/palma-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "sideEffects": false,
  "main": "build/index.js",
  "module": "build/esm/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "draft-convert": "^2.1.8",
    "draft-js": "^0.11.5",
    "prop-types": "^15.7.2",
    "react-input-mask": "^3.0.0-alpha.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-centered": "^5.3.18",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "@svgr/webpack": "^4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-resolve": "^24.9.0",
    "jest-watch-typeahead": "^0.4.2",
    "prettier": "^1.19.1",
    "react-dev-utils": "^10.2.1",
    "react-scripts": "^3.4.1",
    "storybook-readme": "^5.0.8"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --extensions \".js,.jsx\" --out-dir ./build/esm --ignore \"**/*.test.js\",\"**/*.stories.jsx\"  --copy-files --no-copy-ignored",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --extensions \".js,.jsx\" --out-dir ./build --ignore \"**/*.test.js\",\"**/*.stories.jsx\"  --copy-files --no-copy-ignored --presets=@babel/env",
    "release": "npm build && npm publish build --tag latest",
    "test": "node scripts/test.js --watchAll=false",
    "test:watch": "node scripts/test.js",
    "storybook": "start-storybook -p 9009 -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public",
    "prebuild": "rimraf build"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "modulePaths": [],
    "moduleNameMapper": {
      "@styles": "<rootDir>/src/styles",
      "@types": "<rootDir>/src/types",
      "@data": "<rootDir>/src/data"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-optional-chaining"
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@styles": "./src/styles",
            "@types": "./src/types",
            "@data": "./src/data"
          }
        }
      ]
    ]
  }
}
